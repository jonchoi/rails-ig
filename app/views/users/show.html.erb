<div class="user-profile">
  <div class="profile row">
    <div class="col-md-4 avatar">
      <div class="wrapper">
        <img src="https://avatars2.githubusercontent.com/u/49963552?s=400&u=fff79983960e376f7935e04998697ee32d215949&v=4">
      </div>
    </div>

    <div class="col-md-8 basic-info">
      <div class="user-wrapper">
        <h2 class="username"><%= current_user.username %></h2>
        <a class="edit-profile" href="<%= edit_user_path(current_user) %>">
          <button>Edit Profile</button>
        </a>
      </div>
      <ul class="posts">
        <li><span>12</span> posts</li>
        <li><span>122</span> followers</li>
        <li><span>212</span> following</li>
      </ul>
      <h2 class="name"><%= current_user.name %></h2>
      <%=link_to current_user.website, current_user.website, class: 'website' %>
    </div>
    <div class="user-tabs">
      <a class="tab active" href="">
        <i class="fa fa-tv"></i>
        POSTS
      </a>
      <a class="tab" href="">
        <i class="fa fa-tv"></i>
        IGTV
      </a>
      <a class="tab" href="">
        <i class="fa fa-bookmark"></i>
        IGTV
      </a>
      <a class="tab" href="">
        <i class="fa fa-tag"></i>
        TAGGED
      </a>
    </div>
    <div class="user-images">
      <div class="wrapper">
        <img src="https://d2w9rnfcy7mm78.cloudfront.net/5980265/large_102d7f84e32ad063301c3aaa7d7bb5e6.jpg?1579805981?bc=0">
      </div>
      <div class="wrapper">
        <img src="https://d2w9rnfcy7mm78.cloudfront.net/5980278/large_5976012a2311c0a090a15238c2a28aeb.png?1579806088?bc=0">
      </div>
      <div class="wrapper">
        <img src="https://d2w9rnfcy7mm78.cloudfront.net/5979718/large_1a33bf0196841d009444f38cbdbd909e.jpg?1579802787?bc=0">
      </div>
  </div>
</div>